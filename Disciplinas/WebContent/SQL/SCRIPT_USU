CREATE table TB_USUARIO
(
USUARIO_ID INTEGER NOT NULL GENERATED ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1),
USUARIO_NOME VARCHAR(200) NOT NULL,
USUARIO_EMAIL VARCHAR(200) NOT NULL,
USUARIO_MATRICULA VARCHAR(20) NOT NULL unique,
USUARIO_SENHA VARCHAR(200) NOT NULL,
USUARIO_PERIODO VARCHAR(200),
USUARIO_FORMACAO VARCHAR(200),
USUARIO_CARGO VARCHAR(200),
CONSTRAINT primary_key PRIMARY KEY (USUARIO_ID)

);
